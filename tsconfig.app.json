{
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "strict": false,
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}